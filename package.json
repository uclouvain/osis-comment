{
  "name": "osis-comment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --target lib --name osis-comment --entry frontend/main.js --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint frontend",
    "coverage": "jest --coverage",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c frontend/.storybook",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "watch": "vue-cli-service build --target lib --name osis-comment --entry frontend/main.js --mode production --watch"
  },
  "dependencies": {
    "ckeditor4-vue": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/vue": "^6.0.26",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "lint-staged": "^11.1.2",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.2.0",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-template-compiler": "^2.6.14"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "browserslist": [
    "defaults",
    "ie 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "frontend/**/*.{js,vue}",
      "!frontend/main.js"
    ]
  },
  "pperDependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14",
    "vue-i18n": "^8.28.2"
  }
}
