[run]
branch = True
source = osis_comment/osis_comment
omit =
    */migrations/*
    */tests/*
    */management/*
    */admin.py
    */schema.py

[report]
skip_covered = True
show_missing = True
sort = Miss
exclude_lines =
    pragma: no cover
    def __repr__
    if settings.DEBUG
    raise NotImplementedError
